<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PenugasanKewarganegaraan">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp">

        <TextView
            android:id="@+id/txtinklusi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_medium"
            android:padding="20dp"
            android:text="Tugas Pendidikan Kewarganegaraan"
            android:textColor="#000000"
            android:textSize="30sp"
            />

        <TextView
            android:id="@+id/penugasan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/txtinklusi"
            android:fontFamily="@font/poppins_light"
            android:padding="20dp"
            android:text="@string/tugaskewarganegaraan"
            android:textColor="#000000"
            android:textSize="20sp"

            />

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>